      <xsd:element name="Xenomai">
        <xsd:complexType>
          <xsd:attribute name="Compiler" type="xsd:string" use="optional" default="x86_64-linux-gnu-gcc"/>
          <xsd:attribute name="CFLAGS" type="xsd:string" use="optional" default="-I../x86_64-linux-gnu/usr/include/xenomai -D_GNU_SOURCE -D_REENTRANT -Wall -pipe -D__XENO__"/>
          <xsd:attribute name="Linker" type="xsd:string" use="optional" default="x86_64-linux-gnu-gcc"/>
          <xsd:attribute name="LDFLAGS" type="xsd:string" use="optional" default="-lnative -lxenomai -lpthread"/>
          <xsd:attribute name="XenoConfig" type="xsd:string" use="optional"/>
        </xsd:complexType>
      </xsd:element>
